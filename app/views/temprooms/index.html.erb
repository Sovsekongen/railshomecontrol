<p id="notice"><%= notice %></p>

<h1>Temprooms</h1>

<table>
  <thead>
    <tr>
      <th>Rec</th>
      <th>Val</th>
      <th>Name</th>
      <th>Valh</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @temprooms.each do |temproom| %>
      <tr>
        <td><%= temproom.rec %></td>
        <td><%= temproom.val %></td>
        <td><%= temproom.name %></td>
        <td><%= temproom.valH %></td>
        <td><%= link_to 'Show', temproom %></td>
        <td><%= link_to 'Edit', edit_temproom_path(temproom) %></td>
        <td><%= link_to 'Destroy', temproom, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Temproom', new_temproom_path %>
